<div class="album text-muted">
   <div class="container">
      <div class="row">
         <% @products.each do |product| %>
         <div class="card">
		    <strong class="card-text"><%= product.name %></strong>
            <%= image_tag("picture_holder")%>
            <p class="card-text"><%= product.description %></p>
         </div>
         <% end %>
      </div>
	  <div class="row">
	    <div class="col-md-12">
		  <%= paginate @products %>
		</div>
	  </div>
   </div>
</div>
